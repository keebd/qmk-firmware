{
    "manufacturer": "Sebastian Stumpf",
    "keyboard_name": "narfpad",
    "maintainer": "sebastian-stumpf",
    "url": "https://github.com/sebastian-stumpf/narfpad",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "matrix_pins": {
        "cols": ["GP29", "GP28", "GP27", "GP26"],
        "rows": ["GP6", "GP7", "GP0"]
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            {"pin_a": "GP1", "pin_b": "GP2"}
        ]
    },
    "processor": "atmega32u4",
    "bootloader": "caterina",
    "features": {
        "encoder": true
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [0, 1], "x": 1, "y": 0 },
                { "matrix": [0, 2], "x": 2, "y": 0 },
                { "matrix": [0, 3], "x": 3, "y": 0 },
                { "matrix": [1, 1], "x": 1, "y": 1 },
                { "matrix": [1, 2], "x": 2, "y": 1 },
                { "matrix": [1, 3], "x": 3, "y": 1 },
                { "matrix": [2, 0], "x": 0, "y": 2 },
                { "matrix": [2, 1], "x": 1, "y": 2 },
                { "matrix": [2, 2], "x": 2, "y": 2 },
                { "matrix": [2, 3], "x": 3, "y": 2 }
            ]
        }
    }
}